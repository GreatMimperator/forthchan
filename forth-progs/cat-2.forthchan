: clear-stack
	0
	?do
		drop
	loop
;

: cat
	-1 counter !
	begin
		counter @ 1 + counter !		\ increment
		key							\ read key
		dup 13 =					\ check if key is enter (key saved on stack before)
	until
	drop			\ ignore enter
	-1 counter ? -	\ read counter in stack to loop from (counter-1) to 0 (incl)
	do i
		pick			\ clone i element
		emit
	1 -loop			\ minus 1 on every iteration
	counter ?	\ count to clear
	clear-stack
;

cat
cr
